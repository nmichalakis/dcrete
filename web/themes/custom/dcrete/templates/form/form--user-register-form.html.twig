{#
/**
 * @file
 * Template for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 *
 * @ingroup themeable
 */
#}
<div class="row ">
	<div class="col-xs-12 col-md-6">
		<div class="block--user-action">
			<img class="img-fluid mb-5" src="{{ user_login_top }}" alt="user login top icon"/>
			<form{{attributes}}>
				{{ children }}
			</form>
		</div>
	</div>
	<div class="col-xs-12 col-md-6">
		<img class="img-fluid" src="{{ user_login_img }}" alt="user login image"/>

	</div>
</div>


